  "scripts": {
    "dev": "tsx watch server/index.ts",
    "build": "tsc --noEmit false --outDir dist",
    "start": "tsx server/index.ts",
    "start:prod": "node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "node tests/run-tests.js",
    "test:simple": "node tests/run-tests.js simple",
    "test:unit": "node tests/run-tests.js unit",
    "test:integration": "node tests/run-tests.js integration",
    "test:e2e": "node tests/run-tests.js e2e",
    "test:coverage": "c8 --reporter=text --reporter=lcov node tests/run-tests.js",
    "test:watch": "node --import tsx --test --watch tests/**/*.test.js",
    "test:ci": "node tests/run-tests.js --reporter=spec",
    "test:setup": "./scripts/setup-test-env.sh",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "docker:build": "docker build -t contentstageengine .",
    "docker:run": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build -d",
    "docker:stop": "docker-compose -f docker-compose.prod.yml down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "start:prod": "./start-production.sh",
    "start:dev": "./start-dev.sh",
    "health": "./scripts/health-check.sh",
    "fix:setup": "./scripts/fix-production-setup.sh"
  },
